<p id="notice"><%= notice %></p>

<h1>Surveys</h1>

<table class="table">
  <thead>
    <tr>
      <th>Session has student</th>
      <th>Satisfaction</th>
      <th>Contribution</th>
      <th>Conditions</th>
      <th>Domain</th>
      <th>Clarity</th>
      <th>Ability</th>
      <th>Treat</th>
      <th>Method</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @surveys.each do |survey| %>
      <tr>
        <td><%= survey.session_has_student.id %></td>
        <td><%= survey.satisfaction %></td>
        <td><%= survey.contribution %></td>
        <td><%= survey.conditions %></td>
        <td><%= survey.domain %></td>
        <td><%= survey.clarity %></td>
        <td><%= survey.ability %></td>
        <td><%= survey.treat %></td>
        <td><%= survey.method %></td>
        <td><%= link_to 'Show', survey %></td>
        <td><%= link_to 'Edit', edit_survey_path(survey) %></td>
        <td><%= link_to 'Destroy', survey, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Survey', new_survey_path %>
